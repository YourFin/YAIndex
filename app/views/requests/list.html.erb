<%= render "requests/wrapper" do %>
  <table class="table is-fullwidth">
    <tr>
      <th>Created at</th>
      <th>Request</th>
    </tr>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.created_at %></td>
        <td><%= simple_format(request.body) %></td>
      </tr>
    <% end %>
  </table>
<% end %>
